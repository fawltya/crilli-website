{
  "name": "crilli",
  "version": "1.0.0",
  "description": "A blank template to get started with Payload 3.0",
  "license": "MIT",
  "type": "module",
  "scripts": {
    "build": "cross-env NODE_OPTIONS=--no-deprecation next build",
    "dev": "cross-env NODE_OPTIONS=--no-deprecation PAYLOAD_PATCH_TURBOPACK_WARNINGS=false next dev --turbopack",
    "devsafe": "rm -rf .next && cross-env NODE_OPTIONS=--no-deprecation next dev",
    "generate:importmap": "cross-env NODE_OPTIONS=--no-deprecation payload generate:importmap",
    "generate:types": "cross-env NODE_OPTIONS=--no-deprecation payload generate:types",
    "lint": "cross-env NODE_OPTIONS=--no-deprecation next lint",
    "payload": "cross-env NODE_OPTIONS=--no-deprecation payload",
    "start": "cross-env NODE_OPTIONS=--no-deprecation next start"
  },
  "dependencies": {
    "@headlessui/react": "^2.2.2",
    "@payloadcms/db-mongodb": "3.56.0",
    "@payloadcms/db-vercel-postgres": "3.56.0",
    "@payloadcms/next": "3.56.0",
    "@payloadcms/plugin-cloud-storage": "3.56.0",
    "@payloadcms/plugin-seo": "3.56.0",
    "@payloadcms/richtext-lexical": "3.56.0",
    "@payloadcms/storage-vercel-blob": "^3.56.0",
    "@radix-ui/react-separator": "^1.1.4",
    "@radix-ui/react-slot": "^1.2.0",
    "@vercel/analytics": "^1.5.0",
    "@vercel/blob": "^1.0.1",
    "@vercel/speed-insights": "^1.2.0",
    "axios": "^1.9.0",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "cross-env": "^7.0.3",
    "dotenv": "^16.5.0",
    "graphql": "^16.8.1",
    "lucide-react": "^0.503.0",
    "next": "15.3.0",
    "payload": "3.56.0",
    "pg": "8.10.0",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "sharp": "0.32.6",
    "tailwind-merge": "^3.2.0"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.2.0",
    "@tailwindcss/postcss": "^4.1.4",
    "@types/node": "^24.5.2",
    "@types/react": "19.1.0",
    "@types/react-dom": "19.1.2",
    "eslint": "^9.16.0",
    "eslint-config-next": "15.3.0",
    "postcss": "^8.5.3",
    "prettier": "^3.4.2",
    "tailwindcss": "^4.1.4",
    "typescript": "5.7.3",
    "webpack": "^5.99.8"
  },
  "engines": {
    "node": "^18.20.2 || >=20.9.0",
    "pnpm": "^9 || ^10"
  },
  "pnpm": {
    "onlyBuiltDependencies": [
      "sharp"
    ]
  }
}
